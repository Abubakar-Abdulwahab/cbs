@using Parkway.CBS.Core.HelperModels
@{
    Layout.Title = "404";

    Script.Require("CBS.Popper.Script").AtFoot();
    Script.Require("CBS.Bootstrap4.Script").AtFoot();

    Style.Require("CBS.Bootstrap");
    Style.Require("CBS.NPF.Main");
}


<div id="verify-account-one">
    @Html.Partial("Partials\\HeaderPartial", new HeaderObj { })

    <div class="container-fluid error-main-content no-margin no-padding">
        <div class="container error-content-container">
            <div class="container error-content no-padding">
                <div class="container no-padding">
                    <img src="@Url.Content(Html.ThemePath(WorkContext.CurrentTheme,"/Styles/images/404.svg"))" />
                    <h4>Page not found</h4>
                    <p>You may have mistyped the address or the page has been moved</p>
                    <a href="@Url.RouteUrl("P.Index")" class="btn">Go To Homepage</a>
                </div>
            </div>
        </div>
    </div>
    @Html.Partial("Partials\\FooterPartial")
</div>

