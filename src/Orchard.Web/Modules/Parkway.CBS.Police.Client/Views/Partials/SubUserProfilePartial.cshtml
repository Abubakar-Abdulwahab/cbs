@model Parkway.CBS.Police.Core.HelperModels.RegisterPSSUserObj

<div class="form-group">
    <label for="branchName">Branch<span class="required-sym">*</span></label>
    <input type="text" class="form-control" readonly placeholder="Branch Name" value="@Model.LocationInfo.Name" id="branchName" />
    <small class="form-text text-muted" id="nameHint">Please enter the branch name.</small>
</div>
<div class="form-group">
    <label for="subUserFullname">Full Name<span class="required-sym">*</span></label>
    <input type="text" class="form-control" readonly placeholder="SubUser Name" value="@Model.CBSUserName" id="subUserFullname" />
    <small class="form-text text-muted" id="nameHint">Please enter your full name.</small>
</div>
